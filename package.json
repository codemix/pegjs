{
  "name": "peggy-monorepo",
  "version": "1.0.0",
  "description": "Parser generator for JavaScript",
  "private": true,
  "keywords": [
    "parser generator",
    "PEG",
    "PEG.js"
  ],
  "homepage": "https://peggyjs.org/",
  "repository": "peggyjs/peggy",
  "bugs": "https://github.com/peggyjs/peggy/issues",
  "license": "MIT",
  "author": "David Majda <david@majda.cz> (https://majda.cz/)",
  "main": "lib/peg.js",
  "unpkg": "browser/peggy.min.js",
  "jsdelivr": "browser/peggy.min.js",
  "types": "lib/peg.d.ts",
  "bin": "bin/peggy",
  "scripts": {
    "build": "pnpm run -r --if-present --filter ./pkg build",
    "pretest": "npm run build",
    "test": "npm run test:bare",
    "test:bare": "pnpm run -r --if-present --filter ./pkg test",
    "pretest:node": "npm run build",
    "test:node": "nve 10,12,14,15 npm run test:bare",
    "lint": "eslint . --ext .js",
    "benchmark": "./benchmark run",
    "parser": "bin/peggy -o lib/parser.js --format commonjs src/parser.pegjs"
  },
  "devDependencies": {
    "nve": "^13.0.0",
    "open": "^8.0.6",
    "pnpm": "5.18.8"
  },
  "engines": {
    "node": ">=10"
  }
}
