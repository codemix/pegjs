{
  "name": "peggy",
  "version": "1.0.0",
  "description": "Parser generator for JavaScript",
  "keywords": [
    "parser generator",
    "PEG",
    "PEG.js"
  ],
  "homepage": "https://peggyjs.org/",
  "repository": "peggyjs/peggy",
  "bugs": "https://github.com/peggyjs/peggy/issues",
  "license": "MIT",
  "author": "David Majda <david@majda.cz> (https://majda.cz/)",
  "main": "peggy.min.js",
  "types": "lib/peg.d.ts",
  "files": [
    "peggy.min.js",
    "lib/peg.d.ts"
  ],
  "scripts": {
    "parser": "../peggy-cli/bin/peggy -o lib/parser.js --format commonjs src/parser.pegjs",
    "prebuild": "npm run parser",
    "build": "webpack",
    "postbuild": "../../tools/header peggy.min.js && cp peggy.min.js ../../docs/vendor/peggy"
  },
  "devDependencies": {
    "babel-loader": "^8.2.2",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0"
  },
  "engines": {
    "node": ">=10"
  }
}
